<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--Material Circle Image style-->
    <style name="ShapeAppearanceOverlay.CircleImageView" parent="ShapeAppearance.MaterialComponents.MediumComponent">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="SmallTitleGrayBoldCenter">
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="AppTheme.SearchView" parent="Widget.AppCompat.SearchView">
        <item name="android:textSize">14sp</item>
        <item name="android:textColorPrimary">@color/white</item>
        <item name="android:textColorHint">@color/gray_200</item>
        <item name="android:tint">@color/white</item>
    </style>

</resources>